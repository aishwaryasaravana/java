public class ex12 {  
		    int id;  
		    String name;  
		      
	ex12(int i,String n){  
		    id = i;  
		    name = n;  
		    }  
		    void display(){System.out.println(id+" "+name);}  
		   
		    public static void main(String args[]){  
	ex12 s1 = new ex12(111,"Karan");  
	ex12 s2 = new ex12(222,"Aryan");  
		    s1.display();  
		    s2.display();  
	   }  
		}  

