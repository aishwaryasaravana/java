public class ex13 {  
		    int id;  
		    String name;  
	    ex13(int i,String n){  
		    id = i;  
		    name = n;  
	    }  
		      
		    ex13(ex13 s){  
		    id = s.id;  
		    name =s.name;  
		    }  
		    void display(){System.out.println(id+" "+name);}  
		   
		    public static void main(String args[]){  
	ex13 s1 = new ex13(111,"Karan");  
	ex13 s2 = new ex13(s1);  
		    s1.display();  
		    s2.display();  
	   }  
		}  
