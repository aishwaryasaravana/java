public class ex15 {  
		static int count=0;//will get memory only once and retain its value  
		  
		ex15(){  
		count++;  
		System.out.println(count);  
	}  
		  
	public static void main(String args[]){  
		  
	ex15 c1=new ex15();  
	ex15 c2=new ex15();  
	ex15 c3=new ex15();  
		  
		 }  
		}  
